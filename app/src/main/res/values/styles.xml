<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.ButtonWhite" parent="Base.CardView">
        <item name="colorPrimary">@color/white</item>
    </style>

    <style name="roundButton">
        <item name="android:background">@color/material_dynamic_primary80</item>
        <item name="android:radius">15dp</item>
        <item name="android:elevation">15dp</item>
        <item name="android:shadowRadius">100</item>
        <item name="android:shadowColor">@color/black</item>
    </style>

    <style name="home_icons">
        <item name="android:padding">10dp</item>
        <item name="android:layout_margin">10dp</item>
    </style>
</resources>